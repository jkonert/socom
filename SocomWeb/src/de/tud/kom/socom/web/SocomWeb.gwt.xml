<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='socomweb'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- our own css and include files -->
	<stylesheet src='../templates/base/css/base.css' />
	<stylesheet src='../templates/socom/css/addSocom.css' />
	
	<!-- Other module inherits -->
	<inherits name='com.google.gwt.json.JSON' />
	 <!-- for the use of FitImage -->
	<inherits name="com.reveregroup.gwt.imagepreloader.ImagePreloader" />
	 <!-- Inherit sound support -->  	
  	<inherits name="GwtSoundManager2"/>
  	<!-- inherit Resources support -->
  	<inherits name="com.google.gwt.resources.Resources" />
  	<!-- inherit Game-Graph Project 'RaphaelGraph' -->
	<inherits name="de.tud.kom.socom.web.SoComWebGraph"></inherits>
	
	<!-- drag n drop support (http://www.moxiegroup.com/moxieapps/gwt-uploader/quickstart.jsp)-->
	<inherits name="org.moxieapps.gwt.uploader.Uploader"/> 
	
	<!-- Specify the app entry point class. -->
	<entry-point class='de.tud.kom.socom.web.client.SoComWebEntryPoint' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	
	<!-- only compile for one lang and ff and chrome (remove for productive servers!) -->
	<!--
	<set-property name="user.agent" value="gecko1_8,safari" />
	<extend-property name="locale" values="de_DE" />
	 -->
	
	<!-- logger -->
	<!-- 
	<inherits name="com.google.gwt.logging.Logging"/>
	<set-property name="gwt.logging.logLevel" value="INFO"/> 
	-->
	    	
    	    	
</module>
